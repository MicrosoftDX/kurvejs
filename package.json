{
  "name": "kurve",
  "version": "1.0.0",
  "description": "Kurve JS library",
  "license": "MIT",
  "main": "kurve.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MicrosoftDX/kurvejs"
  },
  "devDependencies": {
    "del": "^2.2.0",
    "dts-bundle": "^0.4.3",
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-headerfooter": "^1.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-typescript": "2.13.0",
    "gulp-uglify": "^1.5.3",
    "merge2": "^1.0.2",
    "run-sequence": "^1.1.5",
    "ts-loader": "0.8.1",
    "typescript": "1.8.9"
  },
  "scripts": {
    "build:cleanup": "node build/cleanup",
    "build:dts": "dts-generator --target ES5 --main kurve --project ./ --out dist/kurve.d.ts --module amd",
    "build:bundle": "webpack --config ./webpack.config.js --moduleName kurve",
    "build:bundle-min": "webpack --config ./webpack.config-min.js",
    "build:copy-index": "node build/copy-index",
    "build": "npm run build:cleanup && npm run build:dts  && npm run build:bundle && npm run build:bundle-min && npm run build:copy-index"
  }
}
