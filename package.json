{
  "name": "Kurve",
  "version": "0.5.0",
  "description": "Kurve JS library",
  "license": "MIT",
  "main": "./dist/kurve.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MicrosoftDX/kurvejs"
  },
  "devDependencies": {
    "dts-generator": "1.7.0",
    "fs-extra": "0.26.7",
    "ts-loader": "0.8.1",
    "typescript": "1.8.9",
    "webpack": "1.12.14"
  },
  "scripts": {
    "build:cleanup": "node build/cleanup",
    "build:dts": "dts-generator --name Kurve --main Kurve/src/kurve --project ./ --out dist/kurve.d.ts",
    "build:bundle": "webpack --config ./webpack.config.js",
    "build:bundle-min": "webpack --config ./webpack.config-min.js",
    "build:copy-index": "node build/copy-index",
    "build": "npm run build:cleanup && npm run build:dts  && npm run build:bundle && npm run build:bundle-min && npm run build:copy-index"
  }
}
